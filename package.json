{
    "name": "miku",
    "version": "1.0.0",
    "description": "Miku by Harukoto",
    "main": "dist/index.js",
    "scripts": {
        "format:prettier": "prettier --write .",
        "format:eslint": "eslint . --fix --ignore-pattern \"dist/**\"",
        "format": "run-s format:*",
        "tsc:build": "tsc -p .",
        "tsc:alias": "tsc-alias",
        "git:pull": "git pull",
        "docker:build": "docker compose build",
        "docker:up": "docker compose up -d",
        "build": "run-s tsc:*",
        "start": "node --experimental-global-webcrypto ./dist/index.js",
        "dev": "run-s format build start",
        "bdev": "run-s build start",
        "update": "run-s git:* docker:*"
    },
    "dependencies": {
        "@discord-player/extractor": "^7.0.0",
        "@discordjs/opus": "^0.10.0",
        "@discordjs/voice": "^0.18.0",
        "axios": "^1.7.9",
        "canvacord": "^6.0.2",
        "canvas": "^3.1.0",
        "discord-player": "^7.0.0",
        "discord-player-youtubei": "^1.3.7",
        "discord.js": "14.18.0",
        "dotenv": "^16.4.7",
        "emoji-regex": "^10.4.0",
        "ffmpeg-static": "^5.2.0",
        "glob": "^7.2.3",
        "math-intrinsics": "^1.1.0",
        "mongoose": "^8.9.6",
        "sharp": "^0.33.5",
        "tweetnacl": "^1.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.19.0",
        "@types/glob": "^7.2.0",
        "@types/node": "^22.10.2",
        "@typescript-eslint/eslint-plugin": "^5",
        "@typescript-eslint/parser": "^5",
        "eslint": "^8",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.4.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.1",
        "typescript-eslint": "^8.23.0"
    },
    "private": true,
    "binary": {
        "module_name": "opus",
        "module_path": "./lib/binding/",
        "host": "https://github.com/discordjs/opus/releases/download/"
    }
}
