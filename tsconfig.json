{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@/libraries/*": ["src/libraries/*"],
      "@/handlers/*": ["src/libraries/Classes/Handlers/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
